  "scripts": {
    "assemble": "run-s prettier-check lint build-lib:locl build-lib:prod dist-copy compodoc-custom-theme build-examples:prod",
    "build-examples": "ng b examples",
    "build-examples:prod": "npm run build-examples -- -c production",
    "build-examples:watch": "ng b examples --watch",
    "build-lib": "ng b lib",
    "build-lib:locl": "run-s \"build-lib -- -c locl\" i18n-locl-extract",
    "build-lib:prod": "run-s \"build-lib -- -c production\" dist-copy",
    "build-lib:watch": "run-s dist-copy \"build-lib -- -c dev --watch\"",
    "compodoc": "compodoc",
    "compodoc-custom-theme": "compodoc --extTheme docs/styles/",
    "compodoc:serve": "npm run compodoc-custom-theme -- -s -w --port 8070 --includes ./docs/development",
    "dist-copy": "run-s dist-copy:*",
    "dist-copy:atoms": "tsc -p spec/tsconfig.atom.json",
    "dist-copy:css": "ng build --project=generate-css && npx copyfiles -f \"./.tmp/css/*.{css,svg,woff,woff2}\" dist/bundles/css/",
    "dist-copy:less": "npx copyfiles -f \"src/{nui-charts,nui-charts-*}.less\" dist/sdk/less",
    "dist-copy:locl": "npx copyfiles -f sdk/locale/* dist/sdk/locale",
    "e2e": "start-server-and-test serve-examples:prod 4200 e2e:base",
    "e2e:base": "ng e",
    "e2e:ci": "ng e",
    "e2e:debug": "npm run pree2e:base && node --inspect-brk ./node_modules/protractor/bin/protractor",
    "e2e:full": "npm run e2e:base -- -c full",
    "e2e:gui": "npm run e2e:base -- -c gui",
    "e2e:serve": "npm run e2e:base -- -c serve",
    "e2e:watch": "npx watch \"npm run e2e:base\" src examples spec --watch=1",
    "i18n-locl-extract": "ng extract-i18n lib --format=xlf --output-path=./sdk/locale/ --out-file=messages.en.xlf",
    "lint": "ng l",
    "lint:fix": "ng l --fix",
    "ng": "ng",
    "percy": "percy exec -- npm run visual",
    "percy:ci": "npm run visual:ci && percy upload -v _snapshots",
    "postinstall": "node scripts/remove-deps-constraints.js",
    "pree2e:base": "webdriver-manager update --gecko=false --standalone=false",
    "prettier-check": "yarn run --cwd=../.. prettier-check packages/charts",
    "prettier-fix": "yarn run --cwd=../.. prettier-fix packages/charts",
    "serve-examples": "ng s examples",
    "serve-examples:prod": "npx local-web-server -p 4200 -d sdk/api-docs-ng2/examples --spa index.html",
    "start": "run-p -lr compodoc:serve build-examples:watch",
    "symlink": "run-s symlink:*",
    "symlink:bits": "npx rimraf node_modules/@nova-ui/bits && npx symlink-dir ../bits/dist/ node_modules/@nova-ui/bits",
    "symunlink": "run-s symunlink:*",
    "symunlink:bits": "npx rimraf node_modules/@nova-ui/bits && yarn --check",
    "test": "ng t lib --watch=false --code-coverage",
    "test:dev": "ng t",
    "visual": "npx start-server-and-test serve-examples:prod 4200 visual:base",
    "visual:base": "npm run e2e:base -- --suite visual",
    "visual:ci": "npm run e2e:ci -- --suite visual",
    "visual:full": "npm run visual:base -- -c full",
    "visual:gui": "npm run visual:base -- -c gui",
    "visual:serve": "npm run visual:base -- -c serve"
  },
  "version": "12.0.25-0"
}