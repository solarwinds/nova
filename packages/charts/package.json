{
  "name": "@nova-ui/charts",
  "license": "Apache-2.0",
  "version": "9.33.0-0",
  "scripts": {
    "ng": "ng",
    "start": "run-p -lr compodoc:serve build-examples:watch",
    "assemble": "run-s lint build-lib:locl build-lib:prod compodoc-custom-theme build-examples:prod",
    "build-lib": "ng b lib",
    "build-lib:prod": "run-s \"build-lib -- --prod\" dist-copy",
    "build-lib:locl": "run-s \"ng b lib --configuration=locl\" i18n-locl-extract",
    "build-lib:watch": "run-s dist-copy \"build-lib -- -c dev --watch\"",
    "build-examples": "ng b examples",
    "build-examples:prod": "npm run build-examples -- --prod",
    "build-examples:watch": "npm run build-examples -- --watch",
    "serve-examples": "ng s examples",
    "serve-examples:prod": "npx local-web-server -p 4200 -d sdk/api-docs-ng2/examples --spa index.html",
    "test": "ng t lib --watch=false --code-coverage",
    "test:dev": "ng t",
    "pree2e:base": "webdriver-manager update --gecko=false --standalone=false",
    "e2e:base": "ng e",
    "e2e:ci": "ng e",
    "e2e": "start-server-and-test serve-examples:prod 4200 e2e:base",
    "e2e:serve": "npm run e2e:base -- -c serve",
    "e2e:gui": "npm run e2e:base -- -c gui",
    "e2e:full": "npm run e2e:base -- -c full",
    "e2e:watch": "npx watch \"npm run e2e:base\" src examples spec --watch=1",
    "e2e:debug": "npm run pree2e:base && node --inspect-brk ./node_modules/protractor/bin/protractor",
    "visual:base": "npm run e2e:base -- --suite visual",
    "visual:ci": "npm run e2e:ci -- --suite visual",
    "visual": "npx start-server-and-test serve-examples:prod 4200 visual:base",
    "visual:serve": "npm run visual:base -- -c serve",
    "visual:gui": "npm run visual:base -- -c gui",
    "visual:full": "npm run visual:base -- -c full",
    "compodoc": "compodoc",
    "compodoc-custom-theme": "compodoc --extTheme docs/styles/",
    "compodoc:serve": "npm run compodoc-custom-theme -- -s -w --port 8070 --includes ./docs/development",
    "dist-copy": "run-s dist-copy:*",
    "dist-copy:locl": "cpx sdk/locale/* dist/sdk/locale",
    "dist-copy:atoms": "tsc -p spec/tsconfig.atom.json",
    "dist-copy:less": "cpx \"src/{nui-charts,nui-charts-*}.less\" dist/sdk/less",
    "dist-copy:css": "ng build --project=generate-css && cpx \"./.tmp/css/*.{css,svg,woff,woff2}\" dist/bundles/css/",
    "lint": "ng l",
    "lint:fix": "ng l --fix",
    "i18n-locl-extract": "npx locl extract -s=dist/bundles/nova-ui-charts.umd.js -f=xlf -o=./sdk/locale/messages.en.xlf"
  },
  "peerDependencies": {
    "d3": "^5.0.0",
    "d3-color": "1.3.0",
    "d3-selection-multi": "^1.0.0",
    "d3-time-format": "2.1.3",
    "@nova-ui/bits": "main",
    "@types/d3": "^5.0.0",
    "@types/d3-selection-multi": "^1.0.0"
  },
  "dependencies": {
    "resize-observer-polyfill": "^1.5.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0",
    "@angular-devkit/build-ng-packagr": "^0.900.0",
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/cli": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/localize": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@applitools/eyes-protractor": "1.9.1",
    "@compodoc/compodoc": "1.1.9",
    "@locl/cli": "0.0.1-beta.9",
    "@nova-ui/bits": "main",
    "@types/d3": "^5.7.2",
    "@types/d3-selection-multi": "^1.0.7",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "core-js": "^2.6.9",
    "cpx": "^1.5.0",
    "d3": "^5.9.2",
    "d3-color": "1.3.0",
    "d3-selection-multi": "^1.0.1",
    "d3-time-format": "2.1.3",
    "highlight.js": "9.12.0",
    "jasmine-core": "~3.4.0",
    "jasmine-reporters": "^2.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-teamcity-reporter": "^1.1.0",
    "lodash": "^4.17.12",
    "moment": "^2.16.0",
    "ng-packagr": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.1",
    "rxjs": "^6.5.3",
    "start-server-and-test": "^1.10.5",
    "ts-node": "~8.3.0",
    "tslint": "^5.0.0",
    "typescript": "~3.8.3",
    "zone.js": "~0.10.2"
  }
}
