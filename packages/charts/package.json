{
  "name": "@nova-ui/charts",
  "license": "Apache-2.0",
  "version": "11.10.0-0",
  "author": "SolarWinds <nova-ui@solarwinds.com>",
  "contributors": [
    {
      "name": "SolarWinds Nova Team"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/solarwinds/nova.git"
  },
  "keywords": [
    "chart",
    "nova",
    "nova-ui",
    "solarwinds"
  ],
  "scripts": {
    "ng": "ng",
    "start": "run-p -lr compodoc:serve build-examples:watch",
    "assemble": "run-s lint build-lib:locl build-lib:prod compodoc-custom-theme build-examples:prod",
    "build-lib": "ng b lib",
    "build-lib:prod": "run-s \"build-lib --prod\" dist-copy",
    "build-lib:locl": "run-s \"ng b lib --configuration=locl\" i18n-locl-extract",
    "build-lib:watch": "run-s dist-copy \"build-lib -- -c dev --watch\"",
    "build-examples": "ng b examples",
    "build-examples:prod": "npm run build-examples --prod",
    "build-examples:watch": "ng b examples --watch",
    "serve-examples": "ng s examples",
    "serve-examples:prod": "npx local-web-server -p 4200 -d sdk/api-docs-ng2/examples --spa index.html",
    "test": "ng t lib --watch=false --code-coverage",
    "test:dev": "ng t",
    "pree2e:base": "webdriver-manager update --gecko=false --standalone=false",
    "e2e:base": "ng e",
    "e2e:ci": "ng e",
    "e2e": "start-server-and-test serve-examples:prod 4200 e2e:base",
    "e2e:serve": "npm run e2e:base -- -c serve",
    "e2e:gui": "npm run e2e:base -- -c gui",
    "e2e:full": "npm run e2e:base -- -c full",
    "e2e:watch": "npx watch \"npm run e2e:base\" src examples spec --watch=1",
    "e2e:debug": "npm run pree2e:base && node --inspect-brk ./node_modules/protractor/bin/protractor",
    "percy": "percy exec -- npm run visual",
    "percy:ci": "npm run visual:ci && percy upload -v _snapshots",
    "visual:base": "npm run e2e:base -- --suite visual",
    "visual:ci": "npm run e2e:ci -- --suite visual",
    "visual": "npx start-server-and-test serve-examples:prod 4200 visual:base",
    "visual:serve": "npm run visual:base -- -c serve",
    "visual:gui": "npm run visual:base -- -c gui",
    "visual:full": "npm run visual:base -- -c full",
    "compodoc": "compodoc",
    "compodoc-custom-theme": "compodoc --extTheme docs/styles/",
    "compodoc:serve": "npm run compodoc-custom-theme -- -s -w --port 8070 --includes ./docs/development",
    "dist-copy": "run-s dist-copy:*",
    "dist-copy:locl": "npx copyfiles -f sdk/locale/* dist/sdk/locale",
    "dist-copy:atoms": "tsc -p spec/tsconfig.atom.json",
    "dist-copy:less": "npx copyfiles -f \"src/{nui-charts,nui-charts-*}.less\" dist/sdk/less",
    "dist-copy:css": "ng build --project=generate-css && npx copyfiles -f \"./.tmp/css/*.{css,svg,woff,woff2}\" dist/bundles/css/",
    "lint": "ng l",
    "lint:fix": "ng l --fix",
    "i18n-locl-extract": "npx locl extract -s=dist/bundles/nova-ui-charts.umd.js -f=xlf -o=./sdk/locale/messages.en.xlf",
    "postinstall": "node scripts/remove-deps-constraints.js"
  },
  "peerDependencies": {
    "@nova-ui/bits": "~11.10.0-0",
    "@types/d3": "^5.0.0",
    "@types/d3-selection-multi": "^1.0.0",
    "d3": "^5.0.0",
    "d3-color": "1.3.0",
    "d3-selection-multi": "^1.0.0",
    "d3-time-format": "2.1.3"
  },
  "dependencies": {
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.13",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/animations": "^11.1.2",
    "@angular/cdk": "^11.2.0",
    "@angular/cli": "^11.1.4",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/localize": "^11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@compodoc/compodoc": "^1.1.11",
    "@locl/cli": "^1.0.0",
    "@nova-ui/bits": "~11.10.0-0",
    "@percy/cli": "^1.0.0-beta.41",
    "@percy/protractor": "^2.0.0",
    "@types/d3": "^5.7.2",
    "@types/d3-selection-multi": "^1.0.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "codelyzer": "^6.0.0",
    "core-js": "^2.6.9",
    "d3": "^5.9.2",
    "d3-color": "1.3.0",
    "d3-selection-multi": "^1.0.1",
    "d3-time-format": "2.1.3",
    "eslint": "^7.6.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "highlight.js": "^10.6.0",
    "jasmine-core": "~3.5.0",
    "jasmine-reporters": "^2.3.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.27",
    "ng-packagr": "^11.1.4",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rxjs": "^6.5.3",
    "start-server-and-test": "^1.10.5",
    "ts-node": "~8.3.0",
    "tslib": "^2.1.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "zone.js": "^0.11.3"
  },
  "resolutions": {
    "@compodoc/compodoc/live-server/http-auth": "^3.2.4",
    "@compodoc/compodoc/i18next": "^19.8.5",
    "@compodoc/compodoc/**/pdfkit/crypto-js": "^4.0.0",
    "@compodoc/compodoc/**/glob-parent": "^5.1.2",
    "@compodoc/compodoc/marked": "^2.0.0"
  }
}