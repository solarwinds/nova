{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": false
    },
    "newProjectRoot": "projects",
    "projects": {
        "examples": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "configurations": {
                        "e2e": {
                            "aot": true,
                            "buildOptimizer": true,
                            "extractLicenses": true,
                            "namedChunks": false,
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "vendorChunk": false
                        },
                        "production": {
                            "aot": true,
                            "buildOptimizer": true,
                            "extractLicenses": true,
                            "namedChunks": false,
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "vendorChunk": false
                        }
                    },
                    "defaultConfiguration": "",
                    "options": {
                        "buildOptimizer": false,
                        "extractLicenses": false,
                        "index": "./examples/src/index.html",
                        "main": "./examples/src/index.ts",
                        "namedChunks": true,
                        "optimization": false,
                        "outputPath": "./sdk/api-docs-ng2/examples",
                        "polyfills": "./examples/src/polyfills.ts",
                        "preserveSymlinks": true,
                        "scripts": [],
                        "sourceMap": true,
                        "stylePreprocessorOptions": {
                            "includePaths": ["node_modules"]
                        },
                        "styles": ["./examples/src/styles.less"],
                        "tsConfig": "./examples/src/tsconfig.json",
                        "vendorChunk": true
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "./examples/**/*.ts",
                            "./examples/**/*.html"
                        ]
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "e2e": {
                            "browserTarget": "examples:build:e2e"
                        },
                        "production": {}
                    },
                    "options": {
                        "browserTarget": "examples:build"
                    }
                }
            },
            "projectType": "application",
            "root": "examples",
            "sourceRoot": "examples"
        },
        "examples-e2e": {
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "configurations": {
                        "full": {
                            "devServerTarget": "examples:serve:e2e",
                            "protractorConfig": "./protractor-gui.conf.js"
                        },
                        "gui": {
                            "protractorConfig": "./protractor-gui.conf.js"
                        },
                        "serve": {
                            "devServerTarget": "examples:serve:e2e"
                        }
                    },
                    "options": {
                        "devServerTarget": "",
                        "protractorConfig": "./protractor.conf.js",
                        "suite": "e2e",
                        "webdriverUpdate": false
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["spec/**/*.ts", "spec/**/*.html"]
                    }
                }
            },
            "projectType": "application",
            "root": "spec",
            "sourceRoot": "spec"
        },
        "i18n": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "defaultConfiguration": "",
                    "options": {
                        "allowedCommonJsDependencies": ["lodash"],
                        "buildOptimizer": false,
                        "extractLicenses": false,
                        "index": "./src/index.html",
                        "main": "./src/public-api.ts",
                        "namedChunks": true,
                        "optimization": false,
                        "outputPath": "./.tmp",
                        "sourceMap": true,
                        "tsConfig": "tsconfig.lib.json",
                        "vendorChunk": true
                    }
                }
            },
            "projectType": "library",
            "root": "src",
            "sourceRoot": "src"
        },
        "lib": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "configurations": {
                        "dev": {
                            "project": "ng-package-dev.json"
                        },
                        "locl": {
                            "tsConfig": "./tsconfig.lib.locl.json"
                        },
                        "production": {
                            "project": "ng-package.json",
                            "tsConfig": "./tsconfig.lib.prod.json"
                        }
                    },
                    "options": {
                        "project": "ng-package.json",
                        "tsConfig": "./tsconfig.lib.json"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "i18n:build",
                        "outputPath": "./.tmp"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["./src/**/*.ts", "./src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "configurations": {
                        "coverage": {
                            "codeCoverage": true,
                            "reporters": ["spec"]
                        },
                        "dev": {
                            "reporters": ["spec"],
                            "watch": true
                        }
                    },
                    "options": {
                        "codeCoverage": false,
                        "karmaConfig": "karma.conf.js",
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "preserveSymlinks": true,
                        "sourceMap": false,
                        "stylePreprocessorOptions": {
                            "includePaths": ["src/**/*.less"]
                        },
                        "tsConfig": "tsconfig.spec.json",
                        "watch": false
                    }
                }
            },
            "projectType": "library",
            "root": "src",
            "sourceRoot": "src"
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "nui"
        },
        "@schematics/angular:directive": {
            "prefix": "nui"
        },
        "@angular-eslint/schematics:application": {
            "setParserOptionsProject": true
        },
        "@angular-eslint/schematics:library": {
            "setParserOptionsProject": true
        }
    },
    "version": 1
}
