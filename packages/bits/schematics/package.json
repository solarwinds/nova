{
  "name": "nova-schematics",
  "license": "Apache-2.0",
  "version": "11.0.0-0",
  "scripts": {
    "assemble": "npm run build && npm run test && npm run copy",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*.spec.js",
    "copy": "npm run copy-collection && npm run copy-schematics && npm run copy-utilities",
    "copy-collection": "cpx \"src/collection.json\" ../dist/sdk/schematics/",
    "copy-schematics": "cpx \"src/**/{*.json,index.js,index.js.map,index.d.ts}\" ../dist/sdk/schematics/ && cpx \"src/**/files/**\" ../dist/sdk/schematics/",
    "copy-utilities": "cpx \"src/utility/**/!(*.ts|*.spec.js|*.spec.js.map)\" ../dist/sdk/schematics/utility/ && cpx \"src/utility/**/!(*spec)*.d.ts\" ../dist/sdk/schematics/utility/"
  },
  "keywords": [
    "schematics"
  ]
}
