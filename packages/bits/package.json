    "karma": "^6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "less": "~4.1.1",
    "lodash": "4.17.20",
    "modernizr": "~3.8.0",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.27",
    "ng-packagr": "^11.1.3",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rxjs": "^6.5.3",
    "start-server-and-test": "^1.10.5",
    "ts-node": "~8.6.1",
    "typescript": "~4.1.5",
    "zone.js": "^0.11.3"
  },
  "engines": {
    "node": ">=8.0"
  },
  "resolutions": {
    "@compodoc/compodoc/live-server/http-auth": "^3.2.4",
    "@compodoc/compodoc/i18next": "^19.8.5",
