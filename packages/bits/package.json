{
    "name": "@nova-ui/bits",
    "license": "Apache-2.0",
    "version": "12.0.17-0",
    "author": "SolarWinds <nova-ui@solarwinds.com>",
    "description": "SolarWinds Nova Framework",
    "contributors": [
        {
            "name": "SolarWinds Nova Team"
        }
    ],
    "main": "public_api.js",
    "typings": "public_api.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/solarwinds/nova.git"
    },
    "keywords": [
        "bits",
        "nova",
        "nova-ui",
        "solarwinds"
    ],
    "schematics": "./sdk/schematics/collection.json",
    "ng-update": {
        "migrations": "./sdk/schematics/migration.json"
    },
    "scripts": {
        "ng": "ng",
        "prepublish": "modernizr -c ./src/vendor/modernizr.config.json -d ./node_modules/modernizr/",
        "start": "run-p -lr compodoc:serve build-examples:watch",
        "assemble": "run-s prettier-check lint build-lib:locl build-lib:prod compodoc-custom-theme build-examples:prod build-schematics",
        "prettier-check": "yarn run --cwd=../.. prettier-check packages/bits",
        "prettier-fix": "yarn run --cwd=../.. prettier-fix packages/bits",
        "build-lib": "ng build lib",
        "build-lib:prod": "run-s \"build-lib -- -c production\" dist-copy",
        "build-lib:locl": "run-s \"build-lib -- -c locl\" i18n-locl-extract",
        "build-lib:watch": "run-s dist-copy \"build-lib -- -c dev --watch\"",
        "build-schematics": "npm run --prefix schematics assemble",
        "build-examples": "ng build demo",
        "prebuild-examples": "npm run compile-colors",
        "build-examples:prod": "npm run build-examples -- -c production",
        "build-examples:watch": "run-s \"build-examples --watch\"",
        "serve-examples": "ng serve demo",
        "preserve-examples": "npm run compile-colors",
        "serve-examples:prod": "npx local-web-server -p 4200 -d sdk/api-docs-ng2/examples --spa index.html",
        "test": "ng test lib --watch=false --code-coverage",
        "test:dev": "ng test lib -- -c dev",
        "pree2e:base": "webdriver-manager update --gecko=false --standalone=false",
        "e2e:base": "ng e2e",
        "e2e:ci": "ng e2e",
        "e2e": "start-server-and-test serve-examples:prod 4200 e2e:base",
        "e2e:serve": "npm run e2e:base -- -c serve",
        "e2e:gui": "npm run e2e:base -- -c gui",
        "e2e:full": "npm run e2e:base -- -c full",
        "e2e:watch": "npx watch \"npm run e2e:base\" src demo spec --watch=1",
        "e2e:debug": "npm run pree2e:base && node --inspect-brk ./node_modules/protractor/bin/protractor ./protractor-gui.conf.js",
        "percy": "percy exec -- npm run visual",
        "percy:ci": "npm run visual:ci && percy upload -v _snapshots",
        "percy:gui": "percy exec -- npm run visual:gui",
        "visual:base": "npm run e2e:base -- --suite visual",
        "visual:ci": "npm run e2e:ci -- --suite visual",
        "visual": "start-server-and-test serve-examples:prod 4200 visual:base",
        "visual:serve": "npm run visual:base -- -c serve",
        "visual:gui": "npm run visual:base -- -c gui",
        "visual:watch": "npx watch \"npm run visual:base\" src demo spec --watch=1",
        "visual:full": "npm run visual:base -- -c full",
        "a11y:ci": "npm run e2e:ci -- --suite a11y",
        "a11y:base": "npm run e2e:base -- --suite a11y",
        "a11y": "start-server-and-test serve-examples:prod 4200 a11y:base",
        "compodoc": "compodoc",
        "compodoc-custom-theme": "compodoc --extTheme src/docs/styles/",
        "precompodoc": "node scripts/compile-global-less-styles.js",
        "compodoc:serve": "npm run compodoc-custom-theme -- -s -w --includes ./src/docs/development",
        "dist-copy": "run-s dist-copy:*",
        "dist-copy:locl": "copyfiles -f sdk/locale/* dist/sdk/locale",
        "dist-copy:atoms": "tsc -p spec/tsconfig.atom.json",
        "dist-copy:less": "copyfiles -u 3 \"src/styles/mixins/**/*\" dist/sdk/less/mixins/ && copyfiles -f \"src/styles/{mixins,nui-framework-*}.less\" dist/sdk/less",
        "dist-copy:tslint": "copyfiles \"tslint.*\" dist/sdk/",
        "dist-copy:xliff": "copyfiles -f \"src/locale/*\" dist/sdk/locale",
        "dist-copy:css": "ng build generate-css && copyfiles -f \"./.tmp/css/*.{css,svg,woff,woff2}\" dist/bundles/css/",
        "prexliff": "npm run compile-colors",
        "xliff": "run-s xliff:*",
        "xliff:lib": "ng xi18n lib && ngx-extractor -i \"src/**/*.ts\" -f xlf -o .tmp/messages.xlf && xliffmerge --profile xliffmerge.json",
        "xliff:demo": "ng xi18n demo && ngx-extractor -i \"src/**/*.ts\" \"demo/src/**/*.ts\" -f xlf -o .tmp/messages.xlf && xliffmerge --profile xliffmerge-examples.json",
        "compile-colors": "node ./scripts/colors-conf.js",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix",
        "i18n-locl-extract": "ng extract-i18n lib --format=xlf --output-path=./sdk/locale/ --out-file=messages.en.xlf",
        "postinstall": "cd ."
    },
    "analyze": false,
    "peerDependencies": {
        "@angular/animations": "^12.2.13 || ^13.0.0",
        "@angular/cdk": "^12.2.13 || ^13.0.0",
        "@angular/core": "^12.2.13 || ^13.0.0",
        "@angular/forms": "^12.2.13 || ^13.0.0",
        "@angular/localize": "^12.2.13 || ^13.0.0",
        "@angular/platform-browser": "^12.2.13 || ^13.0.0",
        "@angular/router": "^12.2.13 || ^13.0.0",
        "highlight.js": "^10.6.0",
        "lodash": "^4.17.21",
        "moment": "^2.25.3",
        "moment-timezone": "^0.5.27"
    },
    "dependencies": {
        "tslib": "^2.1.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.2.13",
        "@angular-devkit/build-ng-packagr": "^0.1002.1",
        "@angular-devkit/build-optimizer": "^0.1101.4",
        "@angular-devkit/core": "^12.2.13",
        "@angular-devkit/schematics": "^12.2.13",
        "@angular-devkit/schematics-cli": "^12.2.16",
        "@angular-eslint/builder": "^12.2.0",
        "@angular-eslint/eslint-plugin": "^12.2.0",
        "@angular-eslint/eslint-plugin-template": "^12.2.0",
        "@angular-eslint/schematics": "^12.2.0",
        "@angular-eslint/template-parser": "^12.2.0",
        "@angular/animations": "^12.2.13",
        "@angular/cdk": "^12.2.13",
        "@angular/cli": "^12.2.13",
        "@angular/common": "^12.2.13",
        "@angular/compiler": "^12.2.13",
        "@angular/compiler-cli": "^12.2.13",
        "@angular/core": "^12.2.13",
        "@angular/forms": "^12.2.13",
        "@angular/language-service": "^12.2.13",
        "@angular/localize": "^12.2.13",
        "@angular/platform-browser": "^12.2.13",
        "@angular/platform-browser-dynamic": "^12.2.13",
        "@angular/router": "^12.2.13",
        "@axe-core/webdriverjs": "^4.1.1",
        "@compodoc/compodoc": "1.1.11",
        "@percy/cli": "^1.0.0-beta.74",
        "@percy/protractor": "^2.0.0",
        "@schematics/angular": "^12.2.14",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "2.0.8",
        "@types/lodash": "^4.14.135",
        "@types/node": "^12.11.1",
        "@types/selenium-webdriver": "^3.0.17",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "animate.css": "~3.7.2",
        "apollo-angular": "^2.6.0",
        "@apollo/client": "^3.0.0",
        "bootstrap-less-port": "^0.3.0",
        "codelyzer": "^6.0.1",
        "copyfiles": "^2.4.1",
        "core-js": "^3.19.1",
        "eslint": "^7.32.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.25.0",
        "eslint-plugin-jsdoc": "^37.0.0",
        "eslint-plugin-prefer-arrow": "^1.2.2",
        "graphql": "^15.4.0",
        "highlight.js": "^10.6.0",
        "jasmine-core": "^3.7.1",
        "jasmine-reporters": "^2.3.1",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "karma-spec-reporter": "^0.0.32",
        "less": "~4.1.1",
        "lodash": "^4.17.21",
        "modernizr": "~3.8.0",
        "moment": "^2.25.3",
        "moment-timezone": "^0.5.27",
        "ng-packagr": "^12.2.7",
        "npm-run-all": "^4.1.5",
        "protractor": "~7.0.0",
        "raw-loader": "^4.0.2",
        "rxjs": "^6.5.3",
        "start-server-and-test": "^1.10.5",
        "ts-node": "~8.6.1",
        "typescript": "~4.3.5",
        "zone.js": "~0.11.4"
    },
    "engines": {
        "node": ">=10.0"
    },
    "resolutions": {
        "@compodoc/compodoc/live-server/http-auth": "^3.2.4",
        "@compodoc/compodoc/i18next": "^19.8.5",
        "@compodoc/compodoc/**/pdfkit/crypto-js": "^4.0.0",
        "@compodoc/compodoc/**/glob-parent": "^5.1.2",
        "@compodoc/compodoc/marked": "^2.0.0",
        "@types/eslint": "7.29.0",
        "@types/estree": "0.0.47"
    }
}
