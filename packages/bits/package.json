{
  "name": "@nova-ui/bits",
  "license": "Apache-2.0",
  "version": "11.8.0-0",
  "author": "SolarWinds <nova-ui@solarwinds.com>",
  "description": "SolarWinds Nova Framework",
  "contributors": [
    {
      "name": "SolarWinds Nova Team"
    }
  ],
  "main": "public_api.js",
  "typings": "public_api.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/solarwinds/nova.git"
  },
  "keywords": [
    "bits",
    "nova",
    "nova-ui",
    "solarwinds"
  ],
  "schematics": "./sdk/schematics/collection.json",
  "ng-update": {
    "migrations": "./sdk/schematics/migration.json"
  },
  "scripts": {
    "ng": "ng",
    "prepublish": "modernizr -c ./src/vendor/modernizr.config.json -d ./node_modules/modernizr/",
    "start": "run-p -lr compodoc:serve build-examples:watch",
    "assemble": "run-s lint build-lib:locl build-lib:prod compodoc-custom-theme build-examples:prod build-schematics",
    "build-lib": "ng b lib",
    "build-lib:prod": "run-s \"build-lib --prod\" dist-copy",
    "build-lib:locl": "run-s \"ng b lib --configuration=locl\" i18n-locl-extract",
    "build-lib:watch": "run-s dist-copy \"build-lib -- -c dev --watch\"",
    "build-schematics": "npm run --prefix schematics assemble",
    "build-examples": "ng b demo",
    "prebuild-examples": "npm run compile-colors",
    "build-examples:prod": "npm run build-examples --prod",
    "build-examples:watch": "ng b demo --watch",
    "serve-examples": "ng s demo",
    "preserve-examples": "npm run compile-colors",
    "serve-examples:prod": "npx local-web-server -p 4200 -d sdk/api-docs-ng2/examples --spa index.html",
    "test": "ng t lib --watch=false --code-coverage",
    "test:dev": "ng t lib",
    "pree2e:base": "webdriver-manager update --gecko=false --standalone=false",
    "e2e:base": "ng e",
    "e2e:ci": "ng e",
    "e2e": "start-server-and-test serve-examples:prod 4200 e2e:base",
    "e2e:serve": "npm run e2e:base -- -c serve",
    "e2e:gui": "npm run e2e:base -- -c gui",
    "e2e:full": "npm run e2e:base -- -c full",
    "e2e:watch": "npx watch \"npm run e2e:base\" src demo spec --watch=1",
    "e2e:debug": "npm run pree2e:base && node --inspect-brk ./node_modules/protractor/bin/protractor ./protractor-gui.conf.js",
    "percy": "percy exec -- npm run visual",
    "percy:ci": "npm run visual:ci && percy upload -v _snapshots",
    "percy:gui": "percy exec -- npm run visual:gui",
    "visual:base": "npm run e2e:base -- --suite visual",
    "visual:ci": "npm run e2e:ci -- --suite visual",
    "visual": "start-server-and-test serve-examples:prod 4200 visual:base",
    "visual:serve": "npm run visual:base -- -c serve",
    "visual:gui": "npm run visual:base -- -c gui",
    "visual:watch": "npx watch \"npm run visual:base\" src demo spec --watch=1",
    "visual:full": "npm run visual:base -- -c full",
    "a11y:ci": "npm run e2e:ci -- --suite a11y",
    "a11y:base": "npm run e2e:base -- --suite a11y",
    "a11y": "start-server-and-test serve-examples:prod 4200 a11y:base",
    "compodoc": "compodoc",
    "compodoc-custom-theme": "compodoc --extTheme src/docs/styles/",
    "precompodoc": "node scripts/compile-global-less-styles.js",
    "compodoc:serve": "npm run compodoc-custom-theme -- -s -w --includes ./src/docs/development",
    "dist-copy": "run-s dist-copy:*",
    "dist-copy:locl": "npx copyfiles -f sdk/locale/* dist/sdk/locale",
    "dist-copy:atoms": "tsc -p spec/tsconfig.atom.json",
    "dist-copy:less": "npx copyfiles -u 3 \"src/styles/mixins/**/*\" dist/sdk/less/mixins/ && npx copyfiles -f \"src/styles/{mixins,nui-framework-*}.less\" dist/sdk/less",
    "dist-copy:tslint": "npx copyfiles \"tslint.*\" dist/sdk/",
    "dist-copy:xliff": "npx copyfiles -f \"src/locale/*\" dist/sdk/locale",
    "dist-copy:css": "ng b generate-css && npx copyfiles -f \"./.tmp/css/*.{css,svg,woff,woff2}\" dist/bundles/css/",
    "prexliff": "npm run compile-colors",
    "xliff": "run-s xliff:*",
    "xliff:lib": "ng xi18n lib && ngx-extractor -i \"src/**/*.ts\" -f xlf -o .tmp/messages.xlf && xliffmerge --profile xliffmerge.json",
    "xliff:demo": "ng xi18n demo && ngx-extractor -i \"src/**/*.ts\" \"demo/src/**/*.ts\" -f xlf -o .tmp/messages.xlf && xliffmerge --profile xliffmerge-examples.json",
    "compile-colors": "node ./scripts/colors-conf.js",
    "lint": "ng l",
    "lint:fix": "ng l --fix",
    "i18n-locl-extract": "npx locl extract -s=dist/bundles/nova-ui-bits.umd.js -f=xlf -o=./sdk/locale/messages.en.xlf",
    "postinstall": "cd ."
  },
  "analyze": false,
  "peerDependencies": {
    "@angular/animations": "^11.1.2 || ^12.0.0",
    "@angular/cdk": "^11.1.1 || ^12.0.0",
    "@angular/core": "^11.1.2 || ^12.0.0",
    "@angular/forms": "^11.1.2 || ^12.0.0",
    "@angular/localize": "^11.1.2 || ^12.0.0",
    "@angular/platform-browser": "^11.1.2 || ^12.0.0",
    "@angular/router": "^11.1.2 || ^12.0.0",
    "highlight.js": "^10.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.27"
  },
  "dependencies": {
    "resize-observer-polyfill": "^1.5.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.13",
    "@angular-devkit/build-ng-packagr": "^0.1002.1",
    "@angular-devkit/build-optimizer": "^0.1101.4",
    "@angular-devkit/core": "^11.1.2",
    "@angular-devkit/schematics": "^11.1.4",
    "@angular-devkit/schematics-cli": "^0.1102.0",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/animations": "^11.1.2",
    "@angular/cdk": "^11.1.1",
    "@angular/cli": "^11.1.2",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/language-service": "^11.1.2",
    "@angular/localize": "^11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@axe-core/webdriverjs": "^4.1.1",
    "@compodoc/compodoc": "^1.1.11",
    "@locl/cli": "^1.0.0",
    "@percy/cli": "^1.0.0-beta.41",
    "@percy/protractor": "^2.0.0",
    "@schematics/angular": "^11.1.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.11.1",
