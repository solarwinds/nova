    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-teamcity-reporter": "^1.1.0",
    "less": "~3.8.1",
    "lodash": "^4.17.12",
    "modernizr": "~3.8.0",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.27",
    "ng-packagr": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rxjs": "^6.5.3",
    "start-server-and-test": "^1.10.5",
    "ts-node": "~8.6.1",
    "tslib": "^1.10.0",
    "tslint": "^5.0.0",
    "typescript": "~3.8.3",
    "zone.js": "~0.10.2"
  },
  "engines": {
    "node": ">=8.0"
  }
}
