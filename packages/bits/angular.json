{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": false
    },
    "newProjectRoot": "projects",
    "projects": {
        "demo": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "configurations": {
                        "e2e": {
                            "buildOptimizer": true,
                            "extractLicenses": true,
                            "fileReplacements": [
                                {
                                    "replace": "./demo/src/environments/environment.ts",
                                    "with": "./demo/src/environments/environment.e2e.ts"
                                }
                            ],
                            "namedChunks": false,
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "vendorChunk": false
                        },
                        "i18n": {
                            "localize": ["en-us"]
                        },
                        "production": {
                            "budgets": [],
                            "buildOptimizer": true,
                            "extractLicenses": true,
                            "i18nMissingTranslation": "warning",
                            "namedChunks": false,
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false
                        }
                    },
                    "defaultConfiguration": "",
                    "options": {
                        "allowedCommonJsDependencies": ["lodash"],
                        "buildOptimizer": false,
                        "extractLicenses": false,
                        "index": "./demo/src/index.html",
                        "main": "./demo/src/index.ts",
                        "namedChunks": true,
                        "optimization": false,
                        "outputPath": "./sdk/api-docs-ng2/examples",
                        "polyfills": "./demo/src/polyfills.ts",
                        "scripts": [],
                        "sourceMap": true,
                        "stylePreprocessorOptions": {
                            "includePaths": ["./src/styles"]
                        },
                        "styles": [
                            "src/styles.less",
                            "src/component-styles.less",
                            "src/styles/nui-framework-theme.less",
                            "src/styles/cdk-overlay-override.less",
                            "demo/src/demostyles.less",
                            "src/styles/styles-font-roboto.less"
                        ],
                        "tsConfig": "./demo/src/tsconfig.json",
                        "vendorChunk": true
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "demo:build",
                        "outputPath": "./.tmp-i18n"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["demo/**/*.ts", "demo/**/*.html"]
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "e2e": {
                            "browserTarget": "demo:build:e2e"
                        },
                        "production": {}
                    },
                    "options": {
                        "browserTarget": "demo:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "assets": ["demo/src/assets"],
                        "karmaConfig": "./karma.conf.js",
                        "main": "./demo/src/undefined",
                        "polyfills": "./demo/src/polyfills.ts",
                        "scripts": [],
                        "styles": [
                            "src/styles.less",
                            "src/component-styles.less",
                            "demo/src/demostyles.less"
                        ]
                    }
                }
            },
            "i18n": {
                "locales": {
                    "en-us": {
                        "baseHref": "",
                        "translation": "./demo/src/locale/messages.en-us.xlf"
                    }
                }
            },
            "projectType": "application",
            "root": "demo",
            "sourceRoot": "src"
        },
        "demo-e2e": {
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "configurations": {
                        "full": {
                            "devServerTarget": "demo:serve:e2e",
                            "protractorConfig": "./protractor-gui.conf.js"
                        },
                        "gui": {
                            "protractorConfig": "./protractor-gui.conf.js"
                        },
                        "serve": {
                            "devServerTarget": "demo:serve:e2e"
                        }
                    },
                    "options": {
                        "devServerTarget": "",
                        "protractorConfig": "./protractor.conf.js",
                        "suite": "e2e",
                        "webdriverUpdate": false
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["spec/**/*.ts", "spec/**/*.html"]
                    }
                }
            },
            "projectType": "application",
            "root": "demo",
            "sourceRoot": ""
        },
        "generate-css": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "defaultConfiguration": "",
                    "options": {
                        "buildOptimizer": false,
                        "deleteOutputPath": false,
                        "extractLicenses": false,
                        "index": "index.html",
                        "main": "",
                        "namedChunks": true,
                        "outputPath": "./.tmp-css",
                        "sourceMap": true,
                        "styles": [
                            "src/styles.less",
                            "src/component-styles.less",
                            "src/styles/cdk-overlay-override.less",
                            "src/styles/nui-framework-theme.less",
                            {
                                "bundleName": "styles-font-roboto",
                                "input": "src/styles/styles-font-roboto.less"
                            },
                            {
                                "bundleName": "styles-css-variables",
                                "input": "src/styles/nui-framework-theme.less"
                            }
                        ],
                        "tsConfig": "tsconfig.json",
                        "vendorChunk": true
                    }
                }
            },
            "projectType": "application",
            "root": "./src/styles",
            "sourceRoot": "./src/styles"
        },
        "i18n": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "defaultConfiguration": "",
                    "options": {
                        "allowedCommonJsDependencies": ["lodash"],
                        "buildOptimizer": false,
                        "extractLicenses": false,
                        "index": "./src/index.html",
                        "main": "./src/public_api.ts",
                        "namedChunks": true,
                        "optimization": false,
                        "outputPath": "./.tmp",
                        "sourceMap": true,
                        "tsConfig": "./src/tsconfig.app.json",
                        "vendorChunk": true
                    }
                }
            },
            "projectType": "library",
            "root": "i18n"
        },
        "lib": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "configurations": {
                        "dev": {
                            "project": "ng-package-dev.json"
                        },
                        "locl": {
                            "tsConfig": "./tsconfig.lib.locl.json"
                        },
                        "production": {
                            "project": "ng-package.json",
                            "tsConfig": "./tsconfig.lib.prod.json"
                        }
                    },
                    "options": {
                        "project": "ng-package.json",
                        "tsConfig": "tsconfig.lib.json"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "i18n:build",
                        "outputPath": "./.tmp-i18n"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {},
                    "options": {
                        "browserTarget": "lib:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "configurations": {
                        "coverage": {
                            "codeCoverage": true
                        },
                        "dev": {
                            "reporters": ["spec"],
                            "watch": true
                        }
                    },
                    "options": {
                        "assets": [],
                        "codeCoverage": false,
                        "karmaConfig": "./karma.conf.js",
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "scripts": [],
                        "styles": [],
                        "tsConfig": "src/tsconfig.spec.json",
                        "watch": false
                    }
                }
            },
            "projectType": "library",
            "root": "src",
            "sourceRoot": "src"
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "nui"
        },
        "@schematics/angular:directive": {
            "prefix": "nui"
        }
    },
    "version": 1
}
