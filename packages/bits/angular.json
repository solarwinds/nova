{
    "$schema": "/node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": false,
        "packageManager": "yarn"
    },
    "newProjectRoot": "projects",
    "projects": {
        "demo": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "configurations": {
                        "dev": {
                            "namedChunks": true,
                            "optimization": false,
                            "sourceMap": true
                        },
                        "e2e": {
                            "extractLicenses": true,
                            "fileReplacements": [
                                {
                                    "replace": "./demo/src/environments/environment.ts",
                                    "with": "./demo/src/environments/environment.e2e.ts"
                                }
                            ],
                            "namedChunks": false,
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false
                        },
                        "i18n": {
                            "localize": ["en-us"]
                        },
                        "production": {
                            "budgets": [],
                            "extractLicenses": true,
                            "i18nMissingTranslation": "warning",
                            "namedChunks": false,
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false
                        }
                    },
                    "defaultConfiguration": "",
                    "options": {
                        "loader": {
                            ".html": "text",
                            ".less": "text"
                        },
                        "allowedCommonJsDependencies": [
                            "lodash",
                            "moment/moment"
                        ],
                        "extractLicenses": false,
                        "index": "./demo/src/index.html",
                        "browser": "./demo/src/index.ts",
                        "namedChunks": true,
                        "optimization": false,
                        "outputPath": {
                            "base": "sdk/api-docs-ng2/examples",
                            "browser": ""
                        },
                        "polyfills": ["./demo/src/polyfills.ts"],
                        "scripts": [],
                        "sourceMap": true,
                        "stylePreprocessorOptions": {
                            "includePaths": ["./src/styles"]
                        },
                        "styles": [
                            "./src/styles.less",
                            "./src/component-styles.less",
                            "./src/styles/nui-framework-theme.less",
                            "./src/styles/cdk-overlay-override.less",
                            "./demo/src/demostyles.less",
                            "./src/styles/styles-font-roboto.less"
                        ],
                        "tsConfig": "./demo/src/tsconfig.json"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "demo:build",
                        "outputPath": "./.tmp-i18n"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "./demo/**/*.ts",
                            "./demo/**/*.html"
                        ]
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "dev": {
                            "buildTarget": "demo:build:dev"
                        },
                        "e2e": {
                            "buildTarget": "demo:build:e2e"
                        },
                        "production": {
                            "buildTarget": "demo:build:production"
                        }
                    },
                    "options": {
                        "buildTarget": "demo:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "assets": ["./demo/src/assets"],
                        "karmaConfig": "./karma.conf.js",
                        "main": "./demo/src/module.ts",
                        "polyfills": "./demo/src/polyfills.ts",
                        "scripts": [],
                        "styles": [
                            "./src/styles.less",
                            "./src/component-styles.less",
                            "./demo/src/demostyles.less"
                        ]
                    }
                },
                "e2e": {
                    "builder": "playwright-ng-schematics:playwright",
                    "options": {
                        "devServerTarget": "demo:serve"
                    },
                    "configurations": {
                        "full": {
                            "devServerTarget": "demo:serve:e2e",
                            "protractorConfig": "./protractor-gui.conf.js"
                        },
                        "gui": {
                            "protractorConfig": "./protractor-gui.conf.js"
                        },
                        "serve": {
                            "devServerTarget": "demo:serve:e2e"
                        }
                    }
                }
            },
            "i18n": {
                "locales": {
                    "en-us": {
                        "baseHref": "",
                        "translation": "./demo/src/locale/messages.en-us.xlf"
                    }
                }
            },
            "projectType": "application",
            "root": "./demo",
            "sourceRoot": "./src"
        },
        "generate-css": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "defaultConfiguration": "",
                    "options": {
                        "buildOptimizer": false,
                        "deleteOutputPath": true,
                        "extractLicenses": false,
                        "index": "./index.html",
                        "main": "",
                        "namedChunks": true,
                        "outputPath": "./.tmp-css",
                        "sourceMap": false,
                        "styles": [
                            "./src/styles.less",
                            "./src/component-styles.less",
                            "./src/styles/cdk-overlay-override.less",
                            "./src/styles/nui-framework-theme.less",
                            {
                                "bundleName": "styles-font-roboto",
                                "input": "./src/styles/styles-font-roboto.less"
                            },
                            {
                                "bundleName": "styles-css-variables",
                                "input": "./src/styles/nui-framework-theme.less"
                            }
                        ],
                        "tsConfig": "./src/tsconfig.css.json",
                        "vendorChunk": true
                    }
                }
            },
            "projectType": "application",
            "root": "./src/styles",
            "sourceRoot": "./src/styles"
        },
        "lib": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "configurations": {
                        "dev": {
                            "project": "./ng-package-dev.json"
                        },
                        "production": {
                            "tsConfig": "./tsconfig.lib.prod.json"
                        }
                    },
                    "options": {
                        "project": "./ng-package.json",
                        "tsConfig": "./tsconfig.lib.json"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "i18n:build",
                        "outputPath": "./.tmp-i18n"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["./src/**/*.ts", "./src/**/*.html"]
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {},
                    "options": {
                        "buildTarget": "lib:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "configurations": {
                        "coverage": {
                            "codeCoverage": true,
                            "reporters": [
                                "progress",
                                "coverage",
                                "kjhtml",
                                "junit"
                            ]
                        },
                        "dev": {
                            "reporters": ["spec"],
                            "sourceMap": true,
                            "watch": true
                        }
                    },
                    "options": {
                        "codeCoverage": false,
                        "karmaConfig": "./karma.conf.js",
                        "main": "./src/test.ts",
                        "polyfills": "./src/polyfills.ts",
                        "preserveSymlinks": true,
                        "sourceMap": false,
                        "stylePreprocessorOptions": {
                            "includePaths": ["src/**/*.less"]
                        },
                        "tsConfig": "./tsconfig.spec.json",
                        "watch": false
                    }
                }
            },
            "projectType": "library",
            "root": "./src",
            "sourceRoot": "./src"
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "nui"
        },
        "@schematics/angular:directive": {
            "prefix": "nui"
        }
    },
    "version": 1
}
