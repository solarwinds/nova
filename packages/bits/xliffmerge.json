{
    "xliffmergeOptions": {
        "srcDir": ".tmp",
        "genDir": "src/locale",
        "i18nFile": "messages.xlf",
        "i18nFormat": "xlf",
        "encoding": "UTF-8",
        "defaultLanguage": "en-us",
        "languages": ["en-gb", "de", "en-us", "es", "fr", "it", "pt-br", "ja"],
        "removeUnusedIds": true,
        "useSourceAsTarget": false,
        "beautifyOutput": true,
        "verbose": false,
        "quiet": false
    }
}
