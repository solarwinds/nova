{
  "name": "consumer-web",
  "version": "9.33.0-0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll",
    "start-aot": "ng serve --aot --host 0.0.0.0 --poll",
    "start-aot-fr": "npm run generate-local-xliff && npm run generate-full-fr-xliff && ng serve --host 0.0.0.0 --poll --configuration=production-fr",
    "build": "npm run lint && ng build --prod",
    "build-docker": "npm run lint && ng build --prod --output-path=www",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint consumer-web --fix",
    "e2e": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng e2e --project=consumer-web-e2e",
    "e2e-docker": "ng e2e --project=consumer-web-e2e-docker --host web --port 80 --webdriver-update=false",
    "generate-local-xliff": "ng xi18n && xliff-filter --config xliff-tools-filter.json && ngx-extractor -i src/**/*.ts -f xlf -o .tmp/messages.xlf && xliffmerge --profile xliffmerge.json",
    "generate-full-fr-xliff": "xliff-concat --source node_modules/@solarwinds/nova-bits/sdk/locale/messages.fr.xlf --source src/locale/messages.fr.xlf --dest dist/messages.fr.xlf",
    "report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/cdk": "^8.0.2",
    "@angular/cli": "^8.1.0",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "apollo-link-http": "^1.5.4",
    "core-js": "^2.4.1",
    "karma-teamcity-reporter": "^1.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "rxjs": "^6.5.3",
    "xmldom": "^0.1.27",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.0.2",
    "@angular-builders/dev-server": "^7.0.0",
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/compiler-cli": "^8.1.0",
    "@solarwinds/nova-bits": ">=9.4.0-0",
    "@solarwinds/ts-lint-rules": "^6.0.0",
    "@solarwinds/xliff-tools": "^2.0.0",
    "@types/jasmine": "2.8.9",
    "@types/jasminewd2": "2.0.5",
    "@types/lodash": "^4.14.135",
    "@types/node": "~10.3.2",
    "@types/showdown": "^1.7.5",
    "apollo-angular": "^1.1.0",
    "apollo-cache": "1.1.7",
    "apollo-cache-inmemory": "^1.2.3",
    "apollo-client": "2.3.4",
    "apollo-link-error": "^1.1.0",
    "codelyzer": "^5.0.1",
    "cpx": "^1.5.0",
    "d3": "^4.9.1",
    "d3-selection-multi": "^1.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "highlight.js": "~9.15.8",
    "jasmine-core": "~2.8.0",
    "jasmine-reporters": "~2.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "^0.16.3",
    "protractor": "~5.3.0",
    "showdown": "^1.8.6",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.4.5"
  }
}
