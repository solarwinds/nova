{
  "name": "nova",
  "version": "12.0.11-0",
  "description": "SolarWinds Nova Framework",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/solarwinds/nova.git"
  },
  "author": "SolarWinds <nova-ui@solarwinds.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "in-bits": "npm run --prefix packages/bits",
    "in-charts": "npm run --prefix packages/charts",
    "in-dashboards": "npm run --prefix packages/dashboards",
    "build-watch:bits": "npm run in-bits -- build-lib:watch",
    "build-watch:charts": "npm run in-charts -- build-lib:watch",
    "charts-link:bits": "rimraf packages/charts/node_modules/@nova-ui/bits && symlink-dir packages/bits/dist/ packages/charts/node_modules/@nova-ui/bits",
    "dashboards-link:bits": "rimraf packages/dashboards/node_modules/@nova-ui/bits && symlink-dir packages/bits/dist/ packages/dashboards/node_modules/@nova-ui/bits",
    "dashboards-link:charts": "rimraf packages/dashboards/node_modules/@nova-ui/charts && symlink-dir packages/charts/dist/ packages/dashboards/node_modules/@nova-ui/charts",
    "link:all": "npm run charts-link:bits && npm run dashboards-link:bits && npm run dashboards-link:charts",
    "self-link:bits": "symlink-dir packages/bits/dist/ packages/bits/node_modules/@nova-ui/bits",
    "self-link:charts": "symlink-dir packages/charts/dist/ packages/charts/node_modules/@nova-ui/charts",
    "self-link:dashboards": "symlink-dir packages/dashboards/dist/ packages/dashboards/node_modules/@nova-ui/dashboards",
    "self-link:all": "npm run self-link:bits && npm run self-link:charts && npm run self-link:dashboards",
    "charts-unlink:bits": "rimraf packages/charts/node_modules/@nova-ui/bits && (cd packages/charts && npm install)",
    "dashboards-unlink:bits": "rimraf packages/dashboards/node_modules/@nova-ui/bits && (cd packages/dashboards && npm install)",
    "dashboards-unlink:charts": "rimraf packages/dashboards/node_modules/@nova-ui/charts && (cd packages/dashboards && npm install)",
    "dashboards-unlink": "rimraf packages/dashboards/node_modules/@nova-ui/bits && rimraf packages/dashboards/node_modules/@nova-ui/charts && (cd packages/dashboards && npm install)",
    "charts-unlink": "npm run charts-unlink:bits",
    "unlink:all": "npm run charts-unlink && npm run dashboards-unlink",
    "self-unlink:bits": "rimraf packages/bits/node_modules/@nova-ui/bits",
    "self-unlink:charts": "rimraf packages/charts/node_modules/@nova-ui/charts",
    "self-unlink:dashboards": "rimraf packages/dashboards/node_modules/@nova-ui/dashboards",
    "self-unlink:all": "npm run self-unlink:bits && npm run self-unlink:charts && npm run self-unlink:dashboards",
    "compute-version-ci": "bash scripts/compute-version",
    "set-release-versions-ci": "bash scripts/set-release-versions",
    "set-dependency-versions-ci": "bash scripts/set-dependency-versions",
    "set-source-versions-ci": "bash scripts/set-source-versions",
    "remove-dependencies": "node scripts/remove-dependencies.js",
    "publish-ci": "bash scripts/publish",
    "verify-ci": "bash scripts/verify-published",
    "merge-ci": "bash scripts/merge",
    "deploy-docs-ci": "bash scripts/deploy-docs",
    "clean-docs-ci": "bash scripts/clean-docs",
    "trigger-pipeline-build-ci": "bash scripts/trigger-pipeline-build",
    "bash": "bash scripts/runner",
    "snyk-auth": "snyk auth $SNYK_TOKEN",
    "snyk-test": "snyk test --dev --all-projects --exclude=dist --strict-out-of-sync=false --org=solarwinds || true",
    "snyk-monitor": "snyk monitor --dev --all-projects --exclude=dist --strict-out-of-sync=false --org=solarwinds"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "snyk": "^1.483.0",
    "symlink-dir": "^3.1.0"
  },
  "dependencies": {}
}
